t = int(input())
for _ in range(t):
    x, y = map(int, input().split())
    if x < y:
        if x == y - 1:
            print("yes")
        else:
            print("no")
    elif x == y:
        print("no")
    else:
        if (x - y + 1) % 9 == 0:
            print("yes")
        else:
            print("no")
